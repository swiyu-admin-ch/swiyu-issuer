          "locale": "en-US",
          "logo": {
            "uri": "data:image/png;base64,"
          },
          "description": "Test Credential",
          "background_color": "#007AFF"
        }
      ],
      "vct": "${stage}_university_example_sd_jwt",
      "claims": {
        "type": {
          "mandatory": true,
          "value_type": "string",
          "display": [
            {
              "locale": "de-CH",
              "name": "Abschluss Typ"
            }
          ]
        },
        "name": {
          "mandatory": true,
          "value_type": "string",
          "display": [
            {
              "locale": "de-CH",
              "name": "Diplomtitle"
            }
